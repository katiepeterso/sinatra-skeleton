    <!--- Form to post a new pin -->
      <form method="POST" action="/pins/create" class="col-md-4">
        <fieldset>
          <legend>Post a New Pin</legend>
          <div class="form-group">
            <label for="title1">Pin Title:</label><input type="text" name="title" id="title1">
          </div>
          <div class="form-group">
            <label for="url1">URL:</label><input id="url1" type="url" name="url">
          </div>
        </fieldset>
        <button class="btn btn-default">Post</button>
      </form>
    </div>
    <div class="row">
  </body>